if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    res=0
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    
    if query_name in student_marks:
        avg = ((sum(student_marks[query_name])/3))
        print(f"{avg:.2f}")
            
